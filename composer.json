{
    "name": "keygn/tp6-annotation",
    "description": "Annotation Plus For ThinkPHP6",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Keygn",
            "email": "shada_1992@163.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "topthink/framework": "^6.0",
        "doctrine/annotations": "^1.6",
        "php-di/phpdoc-reader": "^2.1",
        "php": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "keygn\\tp6\\annotation\\": "src"
        }
    },
    "extra": {
        "think": {
            "services": [
                "keygn\\tp6\\annotation\\Service"
            ],
            "config": {
                "annotation": "src/config.php"
            }
        }
    }
}
